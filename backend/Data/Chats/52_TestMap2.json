            "role": "assistant",
            "content": "{\n  \"nodes\": [\n    { \"Id\": 1, \"label\": \"RESERVOIR\", \"Head\": 800 },\n    { \"Id\": 2, \"label\": \"JUNCTION\", \"Elev\": 100 },\n    { \"Id\": 3, \"label\": \"METER\", \"Elev\": 100, \"Demand\": 150, \"Diameter\": 98, \"Lenght\": 14 },\n    { \"Id\": 4, \"label\": \"METER\", \"Elev\": 100, \"Demand\": 150, \"Diameter\": 98, \"Lenght\": 14 },\n    { \"Id\": 5, \"label\": \"METER\", \"Elev\": 100, \"Demand\": 150, \"Diameter\": 98, \"Lenght\": 14 },\n    { \"Id\": 6, \"label\": \"JUNCTION\", \"Elev\": 100 },\n    { \"Id\": 7, \"label\": \"METER\", \"Elev\": 100, \"Demand\": 100, \"Diameter\": 100, \"Lenght\": 12 },\n    { \"Id\": 8, \"label\": \"METER\", \"Elev\": 100, \"Demand\": 100, \"Diameter\": 100, \"Lenght\": 12 },\n    { \"Id\": 9, \"label\": \"JUNCTION\", \"Elev\": 100 },\n    { \"Id\": 10, \"label\": \"METER\", \"Elev\": 100, \"Demand\": 50, \"Diameter\": 102, \"Lenght\": 10 }\n  ],\n  \"edges\": [\n    { \"from\": 1, \"to\": 2, \"label\": \"PIPE\", \"Length\": null, \"Diameter\": null, \"Roughness\": null, \"MinorLoss\": null, \"Status\": null },\n    { \"from\": 2, \"to\": 3, \"label\": \"PIPE\", \"Length\": null, \"Diameter\": null, \"Roughness\": null, \"MinorLoss\": null, \"Status\": null },\n    { \"from\": 3, \"to\": 4, \"label\": \"PIPE\", \"Length\": null, \"Diameter\": null, \"Roughness\": null, \"MinorLoss\": null, \"Status\": null },\n    { \"from\": 4, \"to\": 5, \"label\": \"PIPE\", \"Length\": null, \"Diameter\": null, \"Roughness\": null, \"MinorLoss\": null, \"Status\": null },\n    { \"from\": 5, \"to\": 6, \"label\": \"PIPE\", \"Length\": null, \"Diameter\": null, \"Roughness\": null, \"MinorLoss\": null, \"Status\": null },\n    { \"from\": 6, \"to\": 7, \"label\": \"PIPE\", \"Length\": null, \"Diameter\": null, \"Roughness\": null, \"MinorLoss\": null, \"Status\": null },\n    { \"from\": 7, \"to\": 8, \"label\": \"PIPE\", \"Length\": null, \"Diameter\": null, \"Roughness\": null, \"MinorLoss\": null, \"Status\": null },\n    { \"from\": 8, \"to\": 9, \"label\": \"PIPE\", \"Length\": null, \"Diameter\": null, \"Roughness\": null, \"MinorLoss\": null, \"Status\": null },\n    { \"from\": 9, \"to\": 10, \"label\": \"PIPE\", \"Length\": null, \"Diameter\": null, \"Roughness\": null, \"MinorLoss\": null, \"Status\": null },\n    { \"from\": 10, \"to\": 2, \"label\": \"PIPE\", \"Length\": null, \"Diameter\": null, \"Roughness\": null, \"MinorLoss\": null, \"Status\": null }\n  ]\n}"
