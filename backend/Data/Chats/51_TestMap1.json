            "content": "{\n    \"nodes\": [\n        {\n            \"Id\": 1,\n            \"label\": \"RESERVOIR\",\n            \"Head\": 800\n        },\n        {\n            \"Id\": 2,\n            \"label\": \"JUNCTION\",\n            \"Elev\": 100,\n            \"Demand\": 150\n        },\n        {\n            \"Id\": 3,\n            \"label\": \"JUNCTION\",\n            \"Elev\": 400,\n            \"Demand\": 100\n        },\n        {\n            \"Id\": 4,\n            \"label\": \"JUNCTION\",\n            \"Elev\": 100,\n            \"Demand\": 150\n        }\n    ],\n    \"edges\": [\n        {\n            \"label\": \"PIPE\",\n            \"from\": 1,\n            \"to\": 2\n        },\n        {\n            \"label\": \"PIPE\",\n            \"from\": 2,\n            \"to\": 3\n        },\n        {\n            \"label\": \"PIPE\",\n            \"from\": 3,\n            \"to\": 4\n        }\n    ]\n}"
